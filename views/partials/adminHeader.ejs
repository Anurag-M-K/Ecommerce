<body>

    <!-- Sidebar Start -->
    <div class="sidebar pe-4 pb-3">
        <nav class="navbar bg-secondary navbar-dark">
            <a href="/admin/admin-panel" class="navbar-brand mx-4 mb-3">
                <h3 class="text-primary"><i class="fa fa-admin me-2"></i>Cricket Store</h3>
            </a>
            <div class="d-flex align-items-center ms-4 mb-4">
                <div class="position-relative">
                    <img class="rounded-circle" src="/admin/img/user.jpg" alt="" style="width: 40px; height: 40px;">
                    <div
                        class="bg-success rounded-circle border border-2 border-white position-absolute end-0 bottom-0 p-1">
                    </div>
                </div>
                <div class="ms-3">
                    <h6 class="mb-0">Anurag MK</h6>
                    <span>Admin</span>
                </div>
            </div>
            <div class="navbar-nav w-100">
                <a href="/admin/admin-panel" data-active="adminProduct"
                    class="nav-item nav-link  {{#if dashboard}}active{{/if}}" id="das"><i
                        class="fa fa-tachometer-alt me-2"></i>Dashboard</a>
                <a href="/admin/adminCategory" data-active="adminProduct "
                    class="nav-item nav-link  {{#if category}}active{{/if}}" id="cat"><i
                        class="fa fa-cubes me-2"></i>Categories</a>
                <a href="/admin/product" data-active="adminProduct "
                    class="nav-item nav-link  {{#if product}}active{{/if}}" on id="pro"><i
                        class="	fa fa-sitemap me-2"></i>Products</a>
                <a href="/admin/adminBanner" class="nav-item nav-link  {{#if banner}}active{{/if}}"><i
                        class="fa fa-table me-2"></i>Banner</a>
                <a href="/admin/user-management" data-active="adminProduct "
                    class="nav-item nav-link  {{#if userDetail}}active{{/if}}" id="use"><i
                        class="fa fa-users me-2"></i>Users</a>
                <a href="/admin/coupen" data-active="adminProduct "
                    class="nav-item nav-link  {{#if coupon}}active{{/if}}" id="use"><i
                        class="fa fa-table me-2"></i>Coupons</a>
                <a href="/admin/orders" data-active="adminProduct "
                    class="nav-item nav-link  {{#if order}}active{{/if}}" id="use"><i
                        class="fa fa-chart-bar me-2"></i>Orders</a>
                 <div class="nav-item dropdown">
                    <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown"><i
                            class="fa fa-laptop me-2"></i>Elements</a>
                    <div class="dropdown-menu bg-transparent border-0">
                        <a href="button.html" class="dropdown-item">Buttons</a>
                        <a href="typography.html" class="dropdown-item">Typography</a>
                        <a href="element.html" class="dropdown-item">Other Elements</a>
                    </div>
                </div> 
                <a href="/admin/adminLogout" class="nav-item nav-link"><i class="fa fa-reply me-2"></i>Logout</a>
               
            </div>
        </nav>
    </div>
    <!-- Sidebar End -->



    <script>
        function active(button) {
            $(".active").removeClass("active");
            $(button).addClass("active");
        }
    </script>

    <!-- JavaScript Libraries -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.4.11/d3.min.js"></script>
